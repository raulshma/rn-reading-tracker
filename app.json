{
  "expo": {
    "name": "reading-tracker",
    "slug": "reading-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.raulshma.readingtracker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Reading tracker to keep track of books you are reading",
    "githubUrl": "https://github.com/raulshma/rn-reading-tracker"
  }
}
